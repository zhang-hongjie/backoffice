{
  "name": "backoffice",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules dist typings || true",
    "clean-install": "npm run clean && npm install",
    "start": "webpack-dev-server --config webpack.dev.js --progress --port 3000 --inline",
    "test": "karma start",
    "e2e" : "protractor protractor.conf.js",
    "test-watch": "karma start --no-single-run --auto-watch",
    "lint": "tslint 'src/**/*.ts'",
    "ci": "npm run clean-install && npm run lint && npm test",
    "build": "rm -rf dist/* || true && webpack --config webpack.prod.js --profile --bail",
    "postinstall": "typings install && webdriver-manager update"
  },
  "dependencies": {
    "@angular/common": "2.1",
    "@angular/compiler": "2.1",
    "@angular/core": "2.1",
    "@angular/forms": "2.1",
    "@angular/http": "2.1",
    "@angular/platform-browser": "2.1",
    "@angular/platform-browser-dynamic": "2.1",
    "@angular/platform-server": "2.1",
    "@angular/router": "3.0.0",
    "core-js": "2.4.1",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.7",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25",
    "protractor" : "4.0.10",
    "protractor-html-reporter" : "https://github.com/cbragard/protractor-html-reporter.git",
    "file-saver": "1.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "codelyzer": "0.0.28",
    "compass-mixins": "https://github.com/Igosuki/compass-mixins.git",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "0.25",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "ignore-loader": "0.1.1",
    "imports-loader": "0.6.5",
    "html-webpack-plugin": "^2.8.1",
    "intl": "^1.2.4",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.3.4",
    "jasmine-expect": "3.0.1",
    "jasmine-spec-reporter": "^2.4.0",
    "json-loader": "0.5.4",
    "karma": "1.3.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-matchers": "3.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-notify-send-reporter": "0.0.3",
    "karma-chrome-launcher": "2.0.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "2.1.12",
    "postcss-loader": "0.13.0",
    "raw-loader": "0.5.1",
    "sass-lint": "1.9.1",
    "sasslint-webpack-plugin" : "1.0.4",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "ts-helpers": "^1.1.1",
    "ts-loader": "0.8.2",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "2.0.3",
    "typings": "1.3.2",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.15",
